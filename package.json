  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12.18.2"
  },
  "scripts": {
    "local": "migrate-mongo up && migrate-mongo status &&./node_modules/.bin/nodemon --inspect=0.0.0.0:5001 index.js",
    "docker": "docker-compose up --build",
    "docker:mongo": "docker-compose up app-mongo",
